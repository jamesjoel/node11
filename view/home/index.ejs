<div id="myBox" class="contrainer-fluid" style="margin-top: 20px;">
  <div class="container">
    <div class="row">
      <div class="col-md-3">
        <ul class="list-group">
          <% cateData.forEach(function(info){ %>
            <li class="list-group-item"><a href="#"><%= info.category; %></a></li>
       <% }) %>

          
        </ul>
      </div>
      <div class="col-md-9">
        <ol class="breadcrumb" style="background-color: #000; color: #FFF !important;">
          <li>Popular Items</li>
        </ol>
        <div class="row">
          
          
          
          <% product.forEach(function(x){ %>

          <div class="col-md-4">
            <div class="panel panel-default">
              <div class="panel-heading">
                <h5><%= x.productName %> <small>(Rs. <%= x.productPrice; %>)</small> </h5>
              </div>
              <div class="panel-body">
                <img src="<%= x.imgUrl %>" class="img img-thumbnail" />
              </div>
              <div class="panel-footer">
                <button class="btn btn-primary btn-sm">Buy</button>
                <button class="btn btn-info btn-sm">Add To Cart</button>
              </div>
            </div>
          </div>
        <% }); %>
        </div>
      </div>
    </div>
  </div>
</div>